<div class="right_bar_mobile">
	<h4>メイト一覧</h4>
	<table class="table table-hover">
		<tbody>
			<% if @user_list %>
				<% @user_list.each do |user_list| %>
					<tr class="active">
						<td>
							<a href="/actions/<%= user_list.id %>">
								<%= image_tag(user_list.image, :size => "28x28", :alt => "アイコン")%>
								<%= user_list.name %>
							</a>
						</td>
					</tr>
				<% end %>
			<% end %>
		</tbody>
	</table>
</div>